{
    "name": "airhotel",
    "version": "1.0.0",
    "description": "a fast booking app",
    "main": "src/index.js",
    "scripts": {
        "start": "webpack-dev-server",
        "build": "NODE_ENV='production' webpack",
        "test": "jest --coverage",
        "test:watch": "npm test -- --watchAll",
        "test:clear": "jest --clearCache"
    },
    "jest": {
        "modulePaths": [
            "src"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!**/dist/**",
            "!**/node_modules/**",
            "!**/coverage/**",
            "!**/index.js",
            "!**/webpack.config.js",
            "!**/firestore.js"
        ],
        "moduleNameMapper": {
            "\\.(css|less|scss|sass)$": "identity-obj-proxy",
            "^@Components(.*)$": "<rootDir>/components$1",
            "^@Containers(.*)$": "<rootDir>/containers$1",
            "^@Actions(.*)$": "<rootDir>/actions$1",
            "^@Reducers(.*)$": "<rootDir>/reducers$1"
        }
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    },
    "keywords": [
        "booking",
        "react",
        "app"
    ],
    "devDependencies": {
        "@babel/core": "^7.7.4",
        "@babel/polyfill": "^7.7.0",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-react": "^7.7.4",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.2.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "html-webpack-plugin": "^3.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.9.0",
        "redux": "^4.0.4",
        "redux-thunk": "^2.3.0",
        "style-loader": "^1.0.1",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0"
    },
    "dependencies": {
        "firebase": "^7.5.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-hook-form": "^3.28.13",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2"
    },
    "author": "Michel Kansou",
    "license": "ISC"
}
